<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<StackPane fx:id="rootPane" cache="true" cacheHint="SPEED" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="500.0" minWidth="600.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sokoban.SokobanController">
   <children>
      <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="500.0" minWidth="600.0" styleClass="backgroundColor" stylesheets="@../resources/stylesheet.css">
         <children>
            <HBox>
               <children>
                  <JFXButton focusTraversable="false" onAction="#resetLevel" ripplerFill="#979797" text="Reset Level" textFill="#9e9e9e">
                     <font>
                        <Font name="Gabriola" size="23.0" />
                     </font>
                     <styleClass>
                        <String fx:value="backgroundColor" />
                        <String fx:value="textColor" />
                        <String fx:value="buttonHoverColor" />
                     </styleClass>
                  </JFXButton>
                  <JFXButton focusTraversable="false" layoutX="10.0" layoutY="10.0" onAction="#handleSaveClick" ripplerFill="#979797" text="Save" textFill="#9e9e9e">
                     <font>
                        <Font name="Gabriola" size="23.0" />
                     </font>
                     <styleClass>
                        <String fx:value="backgroundColor" />
                        <String fx:value="textColor" />
                        <String fx:value="buttonHoverColor" />
                     </styleClass>
                  </JFXButton>
                  <JFXButton focusTraversable="false" layoutX="10.0" layoutY="10.0" onAction="#handleLoadClick" ripplerFill="#979797" text="Load" textFill="#9e9e9e">
                     <font>
                        <Font name="Gabriola" size="23.0" />
                     </font>
                     <styleClass>
                        <String fx:value="backgroundColor" />
                        <String fx:value="textColor" />
                        <String fx:value="buttonHoverColor" />
                     </styleClass>
                  </JFXButton>
                  <Region HBox.hgrow="ALWAYS" />
                  <JFXButton focusTraversable="false" onAction="#previousLevel" ripplerFill="#979797" text="Previous Level" textFill="#9e9e9e">
                     <font>
                        <Font name="Gabriola" size="23.0" />
                     </font>
                     <styleClass>
                        <String fx:value="backgroundColor" />
                        <String fx:value="textColor" />
                        <String fx:value="buttonHoverColor" />
                     </styleClass>
                  </JFXButton>
                  <JFXButton focusTraversable="false" onAction="#nextLevel" ripplerFill="#979797" text="Next Level" textFill="#9e9e9e">
                     <font>
                        <Font name="Gabriola" size="23.0" />
                     </font>
                     <styleClass>
                        <String fx:value="backgroundColor" />
                        <String fx:value="textColor" />
                        <String fx:value="buttonHoverColor" />
                     </styleClass>
                  </JFXButton>
               </children>
               <VBox.margin>
                  <Insets left="30.0" right="30.0" top="10.0" />
               </VBox.margin>
            </HBox>
            <Separator styleClass="separatorColor" stylesheets="@../resources/stylesheet.css" VBox.vgrow="ALWAYS">
               <VBox.margin>
                  <Insets left="30.0" right="30.0" />
               </VBox.margin>
            </Separator>
            <AnchorPane fx:id="subRootPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="400.0" minWidth="400.0" VBox.vgrow="ALWAYS">
               <VBox.margin>
                  <Insets bottom="20.0" left="30.0" right="30.0" top="20.0" />
               </VBox.margin>
            </AnchorPane>
         </children>
      </VBox>
   </children>
</StackPane>
